{
  "name": "web-dan-for-js",
  "version": "2.5.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port=4200",
    "build": "ng build --configuration production --base-href \"https://webdan.malme.app/\" && npm run filecopy3 && angular-cli-ghpages",
    "build-actions": "ng build --configuration production --base-href \"https://webdan.malme.app/\" && npm run filecopy3",
    "build-docker:local": "ng build --configuration production --base-href ./ && npm run filecopy3",
    "build-actions:staging": "ng build --configuration production --base-href \"https://st-webdan.malme.app/\" && npm run filecopy3",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:dev": "concurrently \"ng build --delete-output-path=false --watch\" \"webpack --config webpack.electron.dev.config.js --watch\"",
    "start:dev": "electron dist/shell.js",
    "electron:windows": "npm run build:prod && npx electron-builder build --windows",
    "build:prod": "ng build --configuration production --optimization --extract-licenses --build-optimizer && webpack --config webpack.electron.prod.config.js && npm run filecopy1 && npm run filecopy2",
    "start:prod": "electron dist/index.js",
    "filecopy1": "node -e \"require('fs').copyFileSync('dev-app-update.yml', 'dist/dev-app-update.yml')\"",
    "filecopy2": "node -e \"require('fs').copyFileSync('app-update.yml', 'dist/app-update.yml')\"",
    "filecopy3": "node -e \"require('fs').copyFileSync('src/404.html', 'dist/404.html')\"",
    "electron:isas": "npm run build:isas && npx electron-builder build --windows",
    "build:isas": "ng build --configuration production --optimization --extract-licenses --build-optimizer && webpack --config webpack.electron.isas.config.js && npm run filecopy1 && npm run filecopy2"
  },
  "description": "WebDanforJS",
  "author": {
    "name": "MALME INC.",
    "email": "support@structuralengine.com"
  },
  "keywords": [
    "angular",
    "angular 16",
    "typescript",
    "sass"
  ],
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.9",
    "@angular/cdk": "^16.2.9",
    "@angular/common": "^16.2.9",
